<!--components/head/head.wxml-->
<view class="head-box">
  <view class="head-position" style='height: {{statusBarHeight}}px;'></view>
  <view class="">
    <van-icon
      name="arrow-left"
      color="#fff"
      bindtap="back"
      wx:if="{{showBack}}"
    />
    <view class="head">
      <text>{{title}}</text>
    </view>
  </view>
</view>
